
#include <iostream>
using namespace std;
int a,b;
int Sum(int a, int b, int c, int r, int** p, int i, int j);
int main()
{
	int** p;
	int c,r,t;
	int count=-1;
	int xcor=0, ycor=0, tot=0;
	int sum=0;
	
	cin >> t;
	for (int i=0; i<t; i++)
	{
	p=NULL;
	cin >> c;		//column
	cin >> r;
	count++;

	p = new int*[r];
	for (int i = 0; i<r; i++)
		p[i] = new int[c];    			// Making the array
	
	

	for (int i = 0; i < r; i++)
		{

			for (int j = 0; j < c; j++)
			{

				cin >> p[i][j];
	    						// Filling the array
            }
        }
	

        for (int i=0; i < r; i++)
	        for (int j=0; j < c; j++)
			     {
				    sum=Sum(i,j,c,r, p, i, j);
				 if (i==0 && j==0)
	               {
				   tot=sum;
				   ycor=0;
				   xcor=0;
			       }
	             if (sum<tot)
	               {
	               	tot=sum;
	               	ycor=i;
	               	xcor=j;
	               	
				   }
                 };
		
        


    cout << "#" << count << ":   ";
    cout << "(" << xcor << ", " << ycor << ")  ";
    cout << tot << endl;
	for (int k=0; k<r; k++)
		delete []p[k];
    delete []p;
	p=NULL;    
	}
return 0;}


int Sum(int a, int b, int c, int r, int** p, int i, int j)
{
	int tempy=0, tempx=0, temp=0;
	
	for (int k = 0; k < c; k++)
			{
				tempy = tempy + p[i][k];

			};
	for (int k = 0; k < r; k++)
			{
				tempx = tempx + p[k][j];
			};

		temp=tempy + tempx-p[i][j];
return temp;}
