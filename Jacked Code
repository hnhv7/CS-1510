#include <iostream>
using namespace std;

int main()
{
	int** p;
	int a,b,t;
	
	
	
	cin >> t;
	
	for (int i=0; i<t; i++)
	{
	cin >> a;		//column
		cin >> b;
	
		p = new int*[b];
	for (int i = 0; i<b; i++)
		p[i] = new int[a];    			// Making the array
	
	
	
	
	
	for (int i = 0; i < b; i++)
		{
		
			for (int j = 0; j < a; j++)
			{
				int x;
				cin >> x;
				p[i][j] = x;				// Filling the array
}}
	

int xTot=-1;
int yTot=-1;
int temp=-1;
int xCor, yCor, tot;
	for (int i = 0; i < b; i++)
		{ 
		if (xTot == -1)
		  xTot = temp;
		if (temp < xTot)
		 {
		 xTot = temp;
		 xCor = i;
	     };   
		 temp = 0;
			for (int j = 0; j < a; j++)
			{
				temp = temp + p[i][j];
			};
        };
	   temp =-1;		
	for (int i = 0; i < a; i++)
		{  
		  if (yTot == -1)
		  yTot = temp;
		if (temp < yTot)
		 {
		 yTot = temp;
		 yCor = i;
	     };    
		 temp = 0;
			for (int j = 0; j < b; j++)
			{
				temp = temp + p[j][i];
				
			};
        };
        tot = yTot + xTot -p[xCor][yCor];
        
    for (int k=0; k<b; k++)
        delete []p[k];
    delete []p;
    p=NULL;
        
        
        cout << yTot << endl;
        cout << xTot << endl;
        cout << "(" << xCor << ", " << yCor << ")";
        cout << endl << tot;

        
};
return 0;};
